{
	"name": "frontend",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "npm run copy && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:e2e": "playwright test",
		"test": "npm run test:e2e && npm run test:unit -- --run",
		"test:unit": "vitest",
		"clean": "npx run-func ./scripts/content.js clean",
		"copy": "npx run-func ./scripts/content.js copy"
	},
	"devDependencies": {
		"@playwright/test": "^1.45.3",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/enhanced-img": "^0.4.1",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"autoprefixer": "^10.4.20",
		"bits-ui": "^1.0.0-next.58",
		"clsx": "^2.1.1",
		"embla-carousel-svelte": "^8.3.1",
		"lucide-svelte": "^0.454.0",
		"mdsvex": "^0.11.2",
		"run-func": "^3.0.0",
		"sharp": "^0.33.5",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svgo": "^3.3.2",
		"tailwind-merge": "^2.5.4",
		"tailwind-variants": "^0.2.1",
		"tailwindcss": "^3.4.9",
		"typescript": "^5.0.0",
		"vercel": "^37.14.0",
		"vite": "^5.0.3",
		"vitest": "^2.0.4",
		"webdav": "^5.7.1"
	},
	"dependencies": {
		"@inlang/paraglide-sveltekit": "^0.11.1",
		"@std/text": "npm:@jsr/std__text@^1.0.8",
		"@std/toml": "npm:@jsr/std__toml@^1.0.1",
		"embla-carousel-auto-scroll": "^8.3.1",
		"embla-carousel-autoplay": "^8.3.1",
		"leaflet": "^1.9.4",
		"mode-watcher": "^0.4.1",
		"svelte-flags": "^2.0.1",
		"unist-util-visit": "^5.0.0"
	}
}
